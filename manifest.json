{
	"name":"Wayback machine",
	"author":"Internet Archive",
	"description":"Reduce annoying 404 page by automatically checking for an archived copy in the wayback machine",
	"icons":{
		"32":"icon1.png",
		"96":"icon1@2x.png"
	},
	"version":"1",
	"permissions":[
		"tabs",
		"webrequest",
		"http://*/*",
		"https://*/*",
		"<all_urls>"
	],
	"browser_action":{
		"default_icon":{
			"20":"icon.png"
		},
		"default_title":"Wayback machine",
		"default_popup":"popup/popup.html"
	},
	  "background":{
    	"scripts": ["scripts/background.js"]
	}
}